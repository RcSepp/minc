{
  "tasks": [
    {
      "type": "shell",
      "label": "make",
      "command": "make",
      "args": [
        "-j", "8",
        "USE_LLVM=1",
      ],
      "options": {
        "cwd": "${workspaceFolder}",
      },
      "problemMatcher": {
        "base": "$gcc",
        "fileLocation": ["relative", "${workspaceRoot}"],
      },
      "group": {
        "kind": "build",
        "isDefault": true,
      }
    }
  ],
  "version": "2.0.0"
}
