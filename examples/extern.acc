import <builtin.acc>;

stmtdef extern $<BaseType> $I {
	BaseType putsType = FuncType("puts", 0, int, string);

	LLVMTypeRef funcArgTypes[1];
	funcArgTypes[0] = LLVMPointerType(LLVMInt8Type(), 0);
	LLVMTypeRef funcType = LLVMFunctionType(LLVMInt32Type(), funcArgTypes, 1, 0);
	LLVMValueRef func = LLVMAddFunction($1.name, funcType);
	addToScope(parentBlock, $1, putsType, func);
}

extern int puts;

puts("foo\n");

