import paws.extend;
import paws.struct;

export "helloworld-paws"
{
	struct string
	{
		s = "";
	}

	stmt print($E<string>)
	{
	build:
		$0.build();
	run:
		print($0.codegen().s + " from Paws!");
	}

	string expr $L
	{
		str = string();
		str.s = $0.value.substr(1, $0.value.length - 2);
		return str;
	}
}
