import <python37api.acc>;

// Test builtins
print(len);
print(len(__builtins__));
print(PyErr_Occurred());

// Test string
abc = "abc";
gettype(abc);
print(abc, len(abc));

// Test number
cba = 123;
gettype(cba);
print(cba);

// Test '+' operator
a = 213;
b = 312;
print(a + b);

// Test list
l = list();
for (int i = 0; i != 10; i = i + 1) {
	l.append(i);
}
print(l);

// Test list iteration
lr = list();
for i in l {
	lr.insert(0, i);
}
print(lr);

// Test dict
d = dict();
print(d);

// Test import
import sys;
print(sys.path);
from json import [loads];
print(loads);

// Test Python/C interop
int result = abc + 10;
Py_Finalize();
return result;